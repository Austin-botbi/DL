  "source": [
   "import numpy as np, pandas as pd, matplotlib.pyplot as plt\n",
   "from keras.models import Sequential\n",
   "from keras.layers import Conv2D,MaxPool2D,Flatten,Dense\n",
   "from keras.utils import to_categorical\n",
   "from sklearn.model_selection import train_test_split\n",
   "data=pd.read_csv('/content/drive/MyDrive/A_Z Handwritten Data.csv').astype('float32')\n",
   "X,y=data.drop('0',axis=1).values,data['0'].values\n",
   "X=X.reshape(-1,28,28,1);y=to_categorical(y,26)\n",
   "tx,ex,ty,ey=train_test_split(X,y,test_size=0.2)\n",
   "m=Sequential([Conv2D(32,(3,3),activation='relu',input_shape=(28,28,1)),MaxPool2D(2,2),Flatten(),Dense(128,activation='relu'),Dense(26,activation='softmax')])\n",
   "m.compile('adam','categorical_crossentropy',['acc']);m.fit(tx,ty,epochs=1,validation_data=(ex,ey))"
  ],
