  "source": [
   "from keras.datasets import imdb\n",
   "from keras.utils import pad_sequences\n",
   "from keras.models import Sequential\n",
   "from keras.layers import Embedding,SimpleRNN,Dense\n",
   "VOCAB,MAXLEN=10000,200\n",
   "(tx,ty),(ex,ey)=imdb.load_data(num_words=VOCAB)\n",
   "tx,ex=pad_sequences(tx,MAXLEN),pad_sequences(ex,MAXLEN)\n",
   "m=Sequential([Embedding(VOCAB,32),SimpleRNN(32),Dense(1,'sigmoid')])\n",
   "m.compile('rmsprop','binary_crossentropy',['acc']);m.fit(tx,ty,epochs=3,validation_data=(ex,ey))"
  ],
