  "source": [
   "import numpy as np, tensorflow as tf\n",
   "from tensorflow.keras.layers import Input,LSTM,Dense,Embedding\n",
   "from tensorflow.keras.models import Model\n",
   "inp=['I love coding'];tar=['PRP VB NNP']\n",
   "iw=list(set(' '.join(inp).split()));tw=list(set(' '.join(tar).split()+['<sos>','<eos>']))\n",
   "iw2i={w:i for i,w in enumerate(iw)};tw2i={w:i for i,w in enumerate(tw)}\n",
   "enc=np.array([[iw2i[w] for w in s.split()] for s in inp]);dec=np.array([[tw2i[w] for w in s.split()] for s in tar])\n",
   "en_i=Input(shape=(None,));de_i=Input(shape=(None,))\n",
   "en_e=Embedding(len(iw),16)(en_i);de_e=Embedding(len(tw),16)(de_i)\n",
   "en_o,h,c=LSTM(32,return_state=True)(en_e);de_o,_1,_2=LSTM(32,return_sequences=True)(de_e,initial_state=[h,c])\n",
   "out=Dense(len(tw),'softmax')(de_o);m=Model([en_i,de_i],out)\n",
   "m.compile('adam','categorical_crossentropy');print('Model Ready')"
