   "from keras.models import Sequential,Model\n",
   "from keras.layers import Dense,LeakyReLU,Flatten,Reshape,Input\n",
   "from keras.optimizers import Adam\n",
   "import numpy as np\n",
   "g=Sequential([Dense(64,activation='relu',input_dim=100),Dense(784,activation='tanh'),Reshape((28,28,1))])\n",
   "d=Sequential([Flatten(input_shape=(28,28,1)),Dense(64),LeakyReLU(0.2),Dense(1,'sigmoid')])\n",
   "d.compile('adam','binary_crossentropy',metrics=['acc'])\n",
   "z=Input(shape=(100,));img=g(z);d.trainable=False\n",
   "valid=d(img);c=Model(z,valid);c.compile('adam','binary_crossentropy');print('GAN ready')"
